{
  "name": "gazelle",
  "version": "1.0.0",
  "description": "User interface for Gazelle",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wwarby/gazelle-ui-angular.git"
  },
  "keywords": [
    "strava",
    "running",
    "pace",
    "performance",
    "best-efforts",
    "angular"
  ],
  "author": "William Warby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wwarby/gazelle-ui-angular/issues"
  },
  "homepage": "https://github.com/wwarby/gazelle-ui-angular#readme",
  "engines": {
    "node": ">= 10.6.0",
    "npm": ">= 5.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --extract-css --base-href /",
    "build-prod": "ng build --prod --base-href /",
    "deploy-dev": "npm run qc && npm run build && ts-node scripts/deploy dev && rimraf dist",
    "deploy-test": "npm run qc && npm run build && ts-node scripts/deploy test && rimraf dist",
    "deploy-prod": "npm run qc && npm run build-prod && ts-node scripts/deploy prod && rimraf dist",
    "deploy-all": "npm run qc && npm run build && ts-node scripts/deploy dev && ts-node scripts/deploy test && npm run build-prod && ts-node scripts/deploy prod && rimraf dist",
    "docker": "docker build -t gazelle . && docker run -d -p 4200:80 gazelle",
    "sass-lint": "node ./node_modules/sass-lint/bin/sass-lint.js -c .sass-lint.yml -v -q",
    "tslint": "ng lint gazelle --fix && ng lint gazelle --fix",
    "lint": "npm run tslint && npm run sass-lint",
    "test": "ng test --code-coverage",
    "e2e": "ng e2e",
    "qc": "npm run lint && ng test --browsers ChromeHeadless --watch false && ng e2e",
    "sme": "ng build && source-map-explorer dist/main.bundle.js",
    "ncu": "ncu -a -x typescript && npm update",
    "cc": "open ./coverage/index.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.7",
    "@angular/common": "^6.0.7",
    "@angular/compiler": "^6.0.7",
    "@angular/core": "^6.0.7",
    "@angular/forms": "^6.0.7",
    "@angular/http": "^6.0.7",
    "@angular/platform-browser": "^6.0.7",
    "@angular/platform-browser-dynamic": "^6.0.7",
    "@angular/router": "^6.0.7",
    "angular-in-memory-web-api": "^0.6.0",
    "core-js": "^2.5.7",
    "font-awesome": "^4.7.0",
    "normalize.css": "^8.0.0",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.2",
    "@angular/cdk": "^6.3.2",
    "@angular/cli": "~6.1.2",
    "@angular/compiler-cli": "^6.0.7",
    "@angular/language-service": "^6.0.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.112",
    "@types/node": "~10.5.2",
    "codelyzer": "~4.4.2",
    "coveralls": "^3.0.2",
    "fs": "0.0.2",
    "ftp-deploy": "^2.1.2",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.2.0",
    "lodash": "^4.17.10",
    "path": "^0.12.7",
    "protractor": "~5.4.0",
    "sass-lint": "^1.12.1",
    "source-map-explorer": "^1.5.0",
    "ts-node": "^7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.0.1"
  }
}
